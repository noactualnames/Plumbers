program Client;

uses
  Vcl.Forms,
  operatorEntry in 'operatorEntry.pas' {Form1},
  mainWindow in 'mainWindow.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.Run;
end.
